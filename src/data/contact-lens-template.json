
{
    "patientInfo": {
      "title": "Patient Info",
      "icon": "User",
      "fields": [
        { "name": "mrdNo", "label": "Patient ID (Optional)", "type": "input", "placeholder": "e.g., P00123" },
        { "name": "name", "label": "Name", "type": "input", "placeholder": "e.g., Jane Doe" },
        { "name": "dateOfVisit", "label": "Date of Visit", "type": "datepicker" },
        { "name": "age", "label": "Age", "type": "input", "inputType": "number" },
        { "name": "sex", "label": "Sex", "type": "radio", "items": ["Male", "Female", "Other"] },
        { "name": "clPatientProfile", "label": "CL Patient Profile", "type": "select", "options": ["New Fitter", "Existing Patient", "Problem Fitter"], "placeholder": "Select..." },
        { "name": "motivationForCL", "label": "Motivation for CL Wear", "type": "textarea", "rows": 2, "placeholder": "e.g., Sports, Cosmesis" },
        { "name": "lensModalityPreference", "label": "Lens Modality Preference", "type": "select", "options": ["Daily Disposable", "Reusable"], "placeholder": "Select..." }
      ]
    },
    "history": {
      "title": "History & Symptoms",
      "icon": "History",
      "fields": [
        { "name": "chiefComplaint", "label": "Chief Complaint", "type": "textarea", "rows": 2, "placeholder": "e.g., Wants to try contact lenses." },
        { "name": "hpi", "label": "History of Present Illness (HPI)", "type": "textarea", "rows": 3, "placeholder": "Onset, duration, severity of symptoms with CLs." },
        { "name": "previousCLHistory", "label": "Previous CL History", "type": "textarea", "rows": 2, "placeholder": "e.g., Wore soft lenses 5 years ago, discontinued due to dryness." },
        { "name": "currentCLType", "label": "Current CL Type", "type": "input", "placeholder": "e.g., Soft Spherical, RGP" },
        { "name": "currentCLBrand", "label": "Current CL Brand/Parameters", "type": "input", "placeholder": "e.g., Acuvue Oasys, -3.00 DS, BC 8.4, DIA 14.0" },
        { "name": "wearSchedule", "label": "Wear Schedule", "type": "input", "placeholder": "e.g., Daily wear, 12 hours/day" },
        { "name": "replacementSchedule", "label": "Replacement Schedule", "type": "select", "options": ["daily", "bi-weekly", "monthly", "quarterly", "annually"], "placeholder": "Select..." },
        { "name": "careSystemUsed", "label": "Care System Used", "type": "select", "options": ["Multi-purpose solution", "Hydrogen Peroxide", "Other"], "placeholder": "Select..." }
      ]
    },
    "vaRefraction": {
      "title": "Examination (CL Focused)",
      "icon": "Eye",
      "fields": [
        { "name": "vaWithClsOD", "nameOS": "vaWithClsOS", "label": "VA with CLs", "type": "double-field" },
        { "name": "overRefractionOD", "nameOS": "overRefractionOS", "label": "Over-Refraction", "type": "double-field" },
        { "name": "finalVaOverRefractionOD", "nameOS": "finalVaOverRefractionOS", "label": "Final VA with ORx", "type": "double-field" },
        { "name": "manifestRefractionOD", "nameOS": "manifestRefractionOS", "label": "Manifest Refraction (Spectacle Rx)", "type": "double-field" },
        { "name": "k1OD", "nameOS": "k1OS", "label": "Keratometry (K1/Flat)", "type": "double-field" },
        { "name": "k2OD", "nameOS": "k2OS", "label": "Keratometry (K2/Steep)", "type": "double-field" },
        { "name": "hvidOD", "nameOS": "hvidOS", "label": "HVID (mm)", "type": "double-field" },
        { "name": "tbutOD", "nameOS": "tbutOS", "label": "TBUT (sec)", "type": "double-field" }
      ]
    },
    "anteriorSegment": {
      "title": "Lens Fit & Anterior Segment",
      "icon": "Microscope",
      "fields": [
        { "name": "clFitCentrationOD", "nameOS": "clFitCentrationOS", "label": "Centration", "type": "double-select", "options": ["Good", "Slightly Decentered", "Markedly Decentered"] },
        { "name": "clFitMovementOD", "nameOS": "clFitMovementOS", "label": "Movement (with blink)", "type": "double-select", "options": ["Optimal", "Tight (<0.5mm)", "Loose (>1mm)", "Excessive"] },
        { "name": "clFitCoverageOD", "nameOS": "clFitCoverageOS", "label": "Coverage", "type": "double-select", "options": ["Full", "Partial"] },
        { "name": "clFitRotationOD", "nameOS": "clFitRotationOS", "label": "Rotation (for toric)", "type": "double-field" },
        { "name": "slitLampWithCls", "label": "Slit Lamp with CLs IN", "type": "textarea", "rows": 3, "placeholder": "Note overall fit, surface wetting, debris etc." },
        { "name": "stainingOD_details", "nameOS": "stainingOS_details", "label": "Corneal Staining", "type": "double-field", "placeholder": "Location, Severity (e.g., 3&9 SPK, 1+)" },
        { "name": "conjunctivalFindings", "label": "Conjunctival Findings (Lid Eversion)", "type": "textarea", "rows": 2, "placeholder": "e.g., Papillae G1, Follicles, etc." }
      ]
    },
    "plan": {
      "title": "Assessment & Plan",
      "icon": "Edit3",
      "fields": [
        { "name": "assessmentDiagnoses", "label": "Assessment / Diagnoses", "type": "textarea", "rows": 5, "placeholder": "1. Contact Lens Induced Dry Eye OU\n2. Successful CL fit OU (new patient)\n3. Myopia OU" },
        { "name": "plan", "label": "Plan", "type": "textarea", "rows": 5, "placeholder": "1. Dispense trial of Dailies Total 1, daily replacement.\n2. Patient education on proper hygiene, wear schedule, and signs of complications.\n3. RTC 1 week for CL follow-up." },
        { "name": "contactLensRx", "label": "Final Contact Lens Rx", "type": "textarea", "rows": 3, "placeholder": "e.g., OD: Dailies Total 1, -3.00DS, BC 8.5, DIA 14.1. OS: Dailies Total 1, -3.25DS, BC 8.5, DIA 14.1." },
        { "name": "followUpInstructions", "label": "Follow Up Instructions", "type": "textarea", "rows": 3, "placeholder": "e.g., Return in 1 week for contact lens follow-up." }
      ]
    }
}
